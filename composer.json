{
    "name": "indigophp/supervisor-event",
    "description": "Listen to Supervisor events in PHP",
    "license": "MIT",
    "keywords": ["supervisor", "event", "listener"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "guzzlehttp/streams": ">=2.0.0",
        "phpspec/phpspec": "~2.1.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.0"
    },
    "suggest": {
        "guzzlehttp/streams": "Used by Stream listener"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Supervisor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Supervisor\\Stub\\": "stub/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
